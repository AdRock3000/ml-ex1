

data = load('ex1data1.txt');                 % loads data set
X = data(:,1); Y = data(:,2);                % explains X and Y variables  
m = length(Y);                               % number of training examples

plot (X,Y, 'rx', 'MarkerSize', 10);          % plots X,Y data,  rx means red x's as markers
ylabel('Profit in $10,000s');
xlabel('Population in 10,000s');



